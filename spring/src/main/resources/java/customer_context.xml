<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.goodee.ex01.java06.BankAccount" id="bankaccount2">
		<constructor-arg value="010-2222-3333"></constructor-arg>
		<constructor-arg value="100000"></constructor-arg>
	</bean>
	
	<bean class="com.goodee.ex01.java06.Customer" id="customer2">
		<property name="name" value="철수"/>
		<property name="info">
			<map>
				<entry key="tel" value="010-2222-3333"/>
				<entry key="grade" value="gold"/>
			</map>
		</property>
		<property name="bankaccount" ref="bankaccount2"></property>
	</bean>

</beans>
