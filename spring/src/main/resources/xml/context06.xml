<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class="com.goodee.ex01.xml06.Calculator" id="calculator">

	</bean>
	
	<bean class="com.goodee.ex01.xml06.BMICalculator" id="bmicalculator">
		<constructor-arg>
			<ref bean="calculator"/>
		</constructor-arg>
		<constructor-arg>
			<value>180.5</value>
		</constructor-arg>
		<constructor-arg>
			<value>80.5</value>
		</constructor-arg>
	</bean>
	
	<bean class="com.goodee.ex01.xml06.GymMember" id="gymMember">
		<property name="name" value="김나연"/>
		<property name="course">
			<set>
				<value>헬스</value>
				<value>수영</value>
				<value>요가</value>
				<value>필라테스</value>
			</set>
		</property>
		<property name="height" value="180.5"/>
		<property name="weight" value="80.5"/>
		<property name="bmicalculator" ref="bmicalculator"/>
	</bean>

</beans>